    <style name="BottomNavItemStyle" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="BottomNavigationTheme" parent="Theme.AnimePeak">
        <item name="colorPrimary">@color/yellow</item>
        <item name="colorAccent">@color/white</item>
        <item name="itemIconTint">@color/white</item>
        <item name="android:textColorSecondary">@color/white</item>
        <item name="android:colorForeground">@color/white</item>

    </style>
    <style name="MyDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:background">@color/background_main</item>
        <item name="android:textColor">@color/white</item>
        
        <item name="android:textColorPrimary">@color/white</item>
        
        <item name="android:colorControlNormal">@color/background_second</item>
        
        <item name="android:colorControlActivated">@color/yellow</item>
        
        <item name="android:colorControlHighlight">@android:color/transparent</item>
        
        <item name="android:textColorAlertDialogListItem">@color/white</item>

        <item name="dialogCornerRadius">16dp</item>
    </style>
    <style name="Overlay" parent="">

        <item name="colorSurface">@color/background_second</item>


        <item name="boxStrokeWidth">0dp</item>
        <item name="android:dropDownVerticalOffset">10dp</item>
    </style>
    <style name="Theme.AnimePeak" parent="Theme.MaterialComponents.Light.NoActionBar">
        
        <item name="colorPrimary">@color/background_main</item>
        <item name="colorPrimaryVariant">@color/yellow</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/yellow</item>
        <item name="android:textColorSecondary">@color/white</item>

        <item name="colorSecondaryVariant">@color/yellow</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor">?attr/colorPrimary</item>
        

    </style>
    <style name="Theme.AnimePeak.Fullscreen" parent="Theme.AnimePeak">
        <item name="android:actionBarStyle">@style/Widget.Theme.AnimePeak.ActionBar.Fullscreen
        </item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="Theme.AnimePeak.MySplash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">#FFFFFF</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_launcher_foreground</item>
        <item name="splashScreenIconSize">10dp</item>
        <item name="postSplashScreenTheme">@style/Theme.AnimePeak</item>
    </style>
    <style name="ThemeOverlay.AnimePeak.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/yellow</item>
        <item name="fullscreenTextColor">@color/yellow</item>
    </style>
    <style name="Widget.AnimePeak.Spinner" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        
        <item name="boxBackgroundColor">@color/background_second</item>
        <item name="boxStrokeColor">@android:color/transparent</item>


        <item name="materialThemeOverlay">@style/Overlay</item>
    </style>
    <style name="Widget.Theme.AnimePeak.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>
    <style name="Widget.Theme.AnimePeak.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>
    <declare-styleable name="FullscreenAttrs">
        <attr format="color" name="fullscreenBackgroundColor"/>
        <attr format="color" name="fullscreenTextColor"/>
    </declare-styleable>
